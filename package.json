{
  "name": "almighty-ui",
  "version": "0.0.4",
  "description": "A planner front-end for the ALMighty core.",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/almighty/almighty-ui"
  },
  "contributors": [
    {
      "name": "Joshua Wilson",
      "email": "josh@redhat.com"
    },
    {
      "name": "Nimisha",
      "email": "nmukherj@redhat.com"
    }
  ],
  "license": "Apache-2.0",
  "scripts": {
    "build": "webpack --config config/webpack.dev.js",
    "build:prod": "webpack --config config/webpack.prod.js -p",
    "check-coverage": "istanbul check-coverage --statements 23 --branches 5 --functions 9 --lines 24",
    "clean-and-copy": "npm run clean-dist && npm run copy-files",
    "clean-dist": "rimraf dist",
    "copy-files": "cpy src/index.html src/favicon.ico dist",
    "docker:run": "webpack-dev-server --inline --progress --port 8080",
    "lint": "tslint ./src/*",
    "postinstall": "typings install",
    "prestart": "npm run clean-and-copy",
    "prebuild": "npm run clean-and-copy",
    "prebuild:prod": "npm run clean-and-copy",
    "setup": "npm install && npm run validate",
    "start": "webpack-dev-server --inline --progress --host 0.0.0.0 --port 8088",
    "test": "karma start",
    "typings": "typings",
    "validate": "npm-run-all --parallel validate-webpack:* test --serial check-coverage",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "watch:test": "npm test -- --auto-watch --no-single-run",
    
    
    "webdriver-start": "./node_modules/protractor/bin/webdriver-manager update && ./node_modules/protractor/bin/webdriver-manager start",
    "testserver-start": "webpack-dev-server --inline --progress --host 0.0.0.0 --port 8088",
    "protractor": "sleep 15 && ./node_modules/protractor/bin/protractor protractor.config.js",
    "functests": "npm-run-all -p webdriver-start testserver-start protractor"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.6",
    "@angular/compiler": "2.0.0-rc.6",
    "@angular/compiler-cli": "0.6.0",
    "@angular/core": "2.0.0-rc.6",
    "@angular/forms": "2.0.0-rc.6",
    "@angular/http": "2.0.0-rc.6",
    "@angular/platform-browser": "2.0.0-rc.6",
    "@angular/platform-browser-dynamic": "2.0.0-rc.6",
    "@angular/platform-server": "2.0.0-rc.6",
    "@angular/router": "3.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.6",
    "angular2-in-memory-web-api": "0.0.18",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.1",
    "patternfly": "3.8.1",
    "reflect-metadata": "^0.1.4",
    "rxjs": "5.0.0-beta.12",
    "zone.js": "^0.6.23"
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "autoprefixer": "^6.3.7",
    "cpy-cli": "1.0.1",
    "css-loader": "0.23.1",
    "exports-loader": "^0.6.3",
    "extract-text-webpack-plugin": "2.0.0-beta.1",
    "file-loader": "^0.9.0",
    "ghooks": "1.2.4",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "istanbul-instrumenter-loader": "0.2.0",
    "jasmine-core": "2.4.1",
    "karma": "1.3.0",
    "karma-chrome-launcher": "0.2.3",
    "karma-cli": "0.1.2",
    "karma-coverage": "^1.1.1",
    "karma-htmlfile-reporter": "0.2.2",
    "karma-jasmine": "0.3.8",
    "karma-jasmine-html-reporter": "0.2.2",
    "karma-mocha-reporter": "2.2.0",
    "karma-phantomjs-launcher": "1.0.2",
    "karma-sourcemap-loader": "0.3.7",
    "karma-webpack": "1.8.0",
    "mocha": "3.0.2",
    "node-sass": "^3.8.0",
    "npm-run-all": "2.2.0",
    "null-loader": "^0.1.1",
    "opt-cli": "1.4.2",
    "phantomjs-prebuilt": "^2.1.7",
    "postcss-loader": "^0.9.1",
    "precss": "^1.4.0",
    "protractor": "^4.0.8",
    "raw-loader": "^0.5.1",
    "resolve-url-loader": "^1.6.0",
    "rimraf": "2.5.2",
    "sass-loader": "^3.2.2",
    "style-loader": "0.13.1",
    "ts-helpers": "1.1.1",
    "ts-loader": "0.8.1",
    "tslint": "3.7.4",
    "tslint-loader": "2.1.5",
    "typescript": "^1.8.10",
    "typings": "1.3.2",
    "webpack": "2.1.0-beta.22",
    "webpack-dev-middleware": "1.7.0",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-md5-hash": "^0.0.5",
    "webpack-merge": "^0.14.0",
    "webpack-validator": "2.1.3"
  }
}
