{
  "name": "almighty-ui",
  "version": "0.0.3",
  "description": "A planner front-end for the ALMighty core.",
  "homepage": "",
  "repository": {
    "type": "git",
    "url": "https://github.com/almighty/almighty-ui"
  },
  "contributors": [{
      "name" : "Joshua Wilson",
      "email" : "josh@redhat.com"
    },
    {
      "name" : "Nimisha",
      "email" : "nmukherj@redhat.com"
    }],
  "license": "Apache-2.0",

  "scripts": {
    "build": "rimraf dist && webpack --config config/webpack.prod.js --progress --profile --bail",
    "build:prod": "webpack --env.prod -p",
    "check-coverage": "istanbul check-coverage --statements 23 --branches 5 --functions 9 --lines 24",
    "clean-and-copy": "npm run clean-dist && npm run copy-files",
    "clean-dist": "rimraf dist",
    "copy-files": "cpy src/index.html src/favicon.ico dist",
    "lint": "tslint ./src/*",
    "postinstall": "typings install",
    "prestart": "npm run clean-and-copy",
    "prebuild": "npm run clean-and-copy",
    "prebuild:prod": "npm run clean-and-copy",
    "setup": "npm install && npm run validate",
    "start": "webpack-dev-server --inline --progress --port 8088",
    "docker:run": "webpack-dev-server --inline --progress --port 8080",
    "test": "karma start",
    "typings": "typings",
    "validate": "npm-run-all --parallel validate-webpack:* test --serial check-coverage",
    "validate-webpack:dev": "webpack-validator webpack.config.js --env.dev",
    "validate-webpack:prod": "webpack-validator webpack.config.js --env.prod",
    "watch:test": "npm test -- --auto-watch --no-single-run"
  },
  "dependencies": {
    "@angular/common": "2.0.0-rc.5",
    "@angular/compiler": "2.0.0-rc.5",
    "@angular/core": "2.0.0-rc.5",
    "@angular/forms": "0.3.0",
    "@angular/http": "2.0.0-rc.5",
    "@angular/platform-browser": "2.0.0-rc.5",
    "@angular/platform-browser-dynamic": "2.0.0-rc.5",
    "@angular/router": "3.0.0-rc.1",
    "@angular/router-deprecated": "2.0.0-rc.2",
    "@angular/upgrade": "2.0.0-rc.5",
    "angular2-in-memory-web-api": "0.0.15",
    "bootstrap": "^3.3.6",
    "core-js": "^2.4.0",
    "reflect-metadata": "^0.1.4",
    "rxjs": "5.0.0-beta.6",
    "zone.js": "^0.6.12",
    "patternfly":"3.8.1"
  },
  "config": {
    "ghooks": {
      "pre-commit": "opt --in pre-commit --exec \"npm run validate\""
    }
  },
  "devDependencies": {
    "angular2-template-loader": "^0.4.0",
    "chai": "3.5.0",
    "cpy-cli": "1.0.1",
    "css-loader": "0.23.1",
    "extract-text-webpack-plugin": "2.0.0-beta.3",
    "file-loader": "^0.9.0",
    "ghooks": "1.2.4",
    "html-loader": "^0.4.3",
    "html-webpack-plugin": "^2.22.0",
    "istanbul": "0.4.3",
    "jasmine-core": "^2.4.1",
    "karma": "0.13.22",
    "karma-chai": "0.1.0",
    "karma-chrome-launcher": "1.0.1",
    "karma-coverage": "1.0.0",
    "karma-jasmine": "^0.3.8",
    "karma-mocha": "1.0.1",
    "karma-phantomjs-launcher": "^1.0.0",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-webpack": "1.7.0",
    "mocha": "2.5.3",
    "npm-run-all": "2.2.0",
    "null-loader": "^0.1.1",
    "phantomjs-prebuilt": "^2.1.7",
    "opt-cli": "1.4.2",
    "raw-loader": "^0.5.1",
    "rimraf": "2.5.2",
    "style-loader": "0.13.1",
    "ts-loader": "^0.8.1",
    "tslint": "^3.14",
    "typescript": "^1.8.10",
    "typings":"^1.0.4",
    "webpack": "2.1.0-beta.20",
    "webpack-dev-server": "2.1.0-beta.0",
    "webpack-validator": "2.1.3",
    "webpack-merge": "^0.14.0",
    "copy-webpack-plugin": "^3.0.1"
  }
}
